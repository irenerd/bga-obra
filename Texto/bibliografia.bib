@book{tamayo_gomez_hacer_2007,
	title = {Hacer real lo virtual: {Discursos} del desarrollo, tecnologías e historia del {Internet} en {Colombia}},
	shorttitle = {Hacer real lo virtual},
	abstract = {Es interesante preguntarse cómo, en pleno siglo XXI, las relaciones entre la tecnología y el desarrollo siguen siendo imbricaciones inconclusas, inestables e inciertas. Recordemos históricamente cómo en la segunda mitad del siglo XX tuvo lugar el nacimiento de una doctrina (Truman) que fue concebida como el inicio de una nueva era en el manejo y la comprensión de los asuntos internacionales, particularmente concernientes al futuro de los países ubicados al sur del globo.Derivada de las directrices del presidente Harry Truman, aquella doctrina buscó fundamentalmente crear las condiciones necesarias para reproducir en todo el mundo –a través de la ciencia y la tecnología– los rasgos característicos de las sociedades avanzadas de la época: altos niveles de industrialización y urbanización; It is interesting to wonder how, in the 21st century, Relationships between technology and development remain unfinished, unstable and uncertain overlaps. let's remember historically how in the second half of the twentieth century the birth of a doctrine (Truman) that was conceived as the beginning of a new era in the management and understanding of international affairs, particularly concerning the future of the countries located in the south of the globe. Derived from the directives of President Harry Truman, that doctrine fundamentally sought to create the necessary conditions to be replicated around the world - through science and technology - the characteristic features of advanced societies of the time: high levels of industrialization and urbanization;},
	publisher = {Pontificia Universidad Javeriana},
	author = {Tamayo Gomez, Camilo and Delgado, Juan David and Penagos, Julián Enrique},
	year = {2007},
	keywords = {Internet, Alternative development, Colombia, Culture, Desarrollo alternativo, Desarrollo cultura, Development, Estado, History, ICT-technologies information and communication, Paz, Peace, State, Technology, TIC-technologías de la información y comunicación},
}

